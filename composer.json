{
    "name": "jworman/serializer",
    "description": "Allows the (de)serialization of objects.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JWorman\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JWorman\\Serializer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.12",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
